<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<AssemblyVersion>0.0.1</AssemblyVersion>
		<FileVersion>0.0.1</FileVersion>
		<Version>0.0.1</Version>
		<PackageProjectUrl>https://github.com/armindogomes/ticketcode</PackageProjectUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Authors>Armindo Gomes</Authors>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Company />
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<Description>TicketCode is a simple .NET library meant to help generate codes for tickets, coupons, or IDs based on pattern expressions</Description>
		<RepositoryUrl>https://github.com/armindogomes/cnh</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>library ticket pattern random coupon ticketing-system</PackageTags>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\..\README.md" Pack="true" PackagePath="\"/>
	</ItemGroup>
	
	<ItemGroup>
		<PackageReference Include="Antlr4.Runtime.Standard" Version="4.13.1" />
	</ItemGroup>

</Project>